@model Trackii.Models.Admin.Area.AreaEditVm
@{
    ViewData["Title"] = "Nueva Area";
}

<h2>Nueva Area</h2>

<form method="post" action="/Admin/Area/Create">
    @Html.AntiForgeryToken()

    <label>Nombre</label><br />
    <input asp-for="Name" />
    <span asp-validation-for="Name"></span><br /><br />

    <button type="submit">Guardar</button>
    <a href="/Admin/Area">Cancelar</a>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
